generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid()) @map("_id") @db.ObjectId
  email String @unique
  name String?
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Analytics {
  id String @id @default(uuid()) @map("_id") @db.ObjectId
  name String
  userId String @db.ObjectId
}
